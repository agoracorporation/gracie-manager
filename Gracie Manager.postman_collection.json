					"raw": "{\r\n    \"name\": \"{{name}}\",\r\n    \"phoneNumber\":\"{{phoneNumber}}\",\r\n    \"cpf\":\"{{cpf}}\",\r\n    \"address\":\"{{streetAddress}}\",\r\n    \"paymentStatus\":true,\r\n    \"plan\":  {\r\n        \"type\": \"tantofaz\",\r\n        \"startDate\":\"hoje\",\r\n        \"endDate\":\"amanha\",\r\n        \"value\":\"tome\"\r\n    }\r\n}",
					"options": {
